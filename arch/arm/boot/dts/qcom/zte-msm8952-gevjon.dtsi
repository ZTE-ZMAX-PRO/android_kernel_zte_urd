	sx9310-input@28 {
		compatible = "zte,sx9310-input";
		reg = <0x28>;
		cc,irq_gpio = <&msm_gpio 17 0x0>;
		interrupt-parent = <&msm_gpio>;
		interrupts = <17 2>;
		interrupt-names = "sx9310_irq";
		pinctrl-names = "sx9310_active", "sx9310_sleep";
		pinctrl-0 = <&sx9500_int_active>;
		pinctrl-1 = <&sx9500_int_sleep >;
		vdd-supply = <&pm8950_l10>;
		svdd-supply = <&pm8950_l6>;
	};
